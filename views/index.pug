                            img(src=''+imgSrc+'', alt="" data-swiper-parallax-x="-300" data-swiper-parallax-duration="1000")
                            p(data-swiper-parallax-x="-300" data-swiper-parallax-duration="1100") #{slide.description}
                    
            .about #{page.paragraph}
            .offerings
